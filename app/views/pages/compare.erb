                        <td bgcolor="#215300"><%= allPrinciples[3] %></td>
                        <td bgcolor="#215300"><%= allPrinciples[4] %></td>
                        <td bgcolor="#215300"><%= allPrinciples[5] %></td>
                        <td bgcolor="#215300"><%= allPrinciples[6] %></td>
                        <td bgcolor="#215300"><%= allPrinciples[7] %></td>
                        <td bgcolor="#215300"><%= allPrinciples[8] %></td>
                    </tr>
            </tbody>
            </table>
        </div>
        </div>
         <br> 
        <div align='center' id="chartContainer3"><%= @chart3.render() %></div>
        <div align='center' id="chartContainer4"><%= @chart4.render() %></div>
</div>